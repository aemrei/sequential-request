{
  "name": "sequential-request",
  "version": "0.0.3",
  "description": "Sequential requests using simple json/yaml structures",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/packagely/sequential-request.git"
  },
  "keywords": [
    "sequential",
    "request",
    "testing",
    "restful",
    "json"
  ],
  "author": "Emre INEL",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/packagely/sequential-request/issues"
  },
  "homepage": "https://github.com/packagely/sequential-request#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node-fetch": "^2.5.12",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
